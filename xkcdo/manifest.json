{
    "description": "Adds the title text of the comic and a link to the explanation on explainxkcd.com for every xkcd comic",
    "manifest_version": 2,
    "name": "XKCDo",
    "version": "1.0",
    "homepage_url": "https://github.com/YasenPetrov/xkcdo",
    "icons": {
      "48": "icons/xkcdo.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://xkcd.com/*"],
        "js": ["explain.js"]
      }
    ]
    }
}